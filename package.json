{
  "name": "quickstart",
  "version": "1.0.0",
  "description": "Structure to quickly start frontend projects in angularjs with bootstrap themes",
  "main": "index.js",
  "scripts": {
    "fonts": "cp -R node_modules/bootstrap/fonts fonts",
    "pro": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.dev.js --watch",
    "back": "json-server db.json",
    "start": "npm-run-all --parallel dev back"
  },
  "author": {
    "name": "Daniel Herrero",
    "url": "https://www.linkedin.com/in/daniel-herrero-tom%C3%A1s-7781b143/"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/dherrero/quickstart"
  },
  "bugs": {
    "url": "https://github.com/dherrero/quickstart/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "json-server": "^0.10.0",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.1",
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-aria": "^1.6.4",
    "angular-moment": "^1.0.1",
    "angular-moment-picker": "^0.10.1",
    "angular-toastr": "1.7.0",
    "angular-translate": "2.10.0",
    "angular-translate-loader-url": "2.10.0",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router-anim-in-out": "^1.0.2",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "ng-dialog": "^1.3.0"
  }
}
